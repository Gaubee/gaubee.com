---
import ScaffoldLayout from "@/layouts/ScaffoldLayout.astro";
import RootLayout from "@/layouts/RootLayout.astro";

// Re-defining props here as it's not exported from the base layout.
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<ScaffoldLayout title={title}>
  <slot />
</ScaffoldLayout>

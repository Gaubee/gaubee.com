{
  "name": "@gaubee/com",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "dist": "tsc",
    "dist:watch": "pnpm dist --watch",
    "11ty": "eleventy --config=eleventy.config.ts",
    "11ty:watch": "node --watch ./node_modules/@11ty/eleventy/cmd.cjs  --config=eleventy.config.ts --watch --serve --port=3000",
    "bundle": "rollup -c",
    "bundle:watch": "pnpm bundle --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "build": "pnpm dist --noCheck && pnpm analyze && pnpm bundle && pnpm 11ty",
    "test": ""
  },
